destination = int(input("please select your destination \n 1.international \n 2.domestic \n"))
if(destination != 1) and (destination != 2):
    print("invalid input for destination")
else:
    ticket = int(input("select type of ticket \n 1.first class \n 2.economy \n"))
    if(ticket != 1) and (ticket != 2):
        print("invalid input for ticket")
    else:
        if(destination == 1 and ticket == 1):
            print("you got discount 15%")
        elif(destination == 1 and ticket == 2):
            print("you got discount 10%")
        elif(destination == 2 and ticket == 1):
            print("you got discount 5%")
        elif(destination == 2 and ticket == 2):
            print("you got no discount")
